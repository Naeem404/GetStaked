{
  "expo": {
    "name": "Get Staked",
    "slug": "get-staked",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "getstaked",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.getstaked.app",
      "associatedDomains": [
        "applinks:getstaked.ca",
        "applinks:get-staked.vercel.app"
      ],
      "infoPlist": {
        "NSCameraUsageDescription": "Get Staked needs camera access to capture daily proof photos for your stake pools.",
        "NSPhotoLibraryUsageDescription": "Get Staked needs photo library access to select proof images.",
        "NSMicrophoneUsageDescription": "Get Staked may use the microphone for video proof recording.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0A0A0A",
        "foregroundImage": "./assets/images/android-icon-foreground.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.getstaked.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "getstaked.ca",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "get-staked.vercel.app",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "backgroundColor": "#0A0A0A"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0A0A0A"
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Get Staked needs camera access to capture daily proof photos for your stake pools.",
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Get Staked needs photo library access to select proof images.",
          "cameraPermission": "Get Staked needs camera access to capture proof photos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6f7690fe-1522-4886-a0c8-d0cab2942e1f"
      }
    }
  }
}
